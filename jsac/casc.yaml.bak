jenkins:
  securityRealm:
    activeDirectory:
      domains:
        - name: "acme"
          servers: "ad1.acme.com:123,ad2.acme.com:456"
          site: "site"
          bindName: "admin"
          bindPassword: "${BIND_PASSWORD}"
          tlsConfiguration: TRUST_ALL_CERTIFICATES
      groupLookupStrategy: "RECURSIVE"
      removeIrrelevantGroups: true
      customDomain: true
      cache:
        size: 500
        ttl: 600
      startTls: true  
unclassified:
  location:
    url: http://*:8081